{
	"parserOptions": {
		"ecmaVersion": 2019,
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"node": true,
		"browser": true
	},
	"plugins": [
		"import",
		"svelte3"
	],
	"extends": [
		"eslint:recommended",
		"plugin:import/recommended"
	],
	"overrides": [
		{
			"files": [
				"**/*.svelte"
			],
			"processor": "svelte3/svelte3"
		}
	]
}
